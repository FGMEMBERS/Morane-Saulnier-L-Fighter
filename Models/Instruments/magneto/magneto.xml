<?xml version="1.0" encoding="UTF-8"?>

<!-- ########################################

Magneto switch improved from old FS2004 Freeware Cessna 120 of Paul Clausen's

##########################################-->

<PropertyList>

<!-- magneto -->

<path>magneto.ac</path>

<animation>
  <type>pick</type>
  <object-name>on-mags</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine/magnetos</property>
    <step>1.0</step>
    <min>0</min>
    <max>3</max>
    <wrap>0</wrap>
    </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
  <object-name>off-mags</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine/magnetos</property>
    <step>-1.0</step>
    <min>0</min>
    <max>3</max>
    <wrap>0</wrap>
    </binding>
  </action>
</animation>

  <animation>
  <type>rotate</type>
  <object-name>switch</object-name>
  <property>/controls/engines/engine/magnetos</property>
  <interpolation>
   <entry><ind> 0</ind><dep> 0</dep></entry>
   <entry><ind> 1</ind><dep> -30</dep></entry>
   <entry><ind> 2</ind><dep> -60</dep></entry>
   <entry><ind> 3</ind><dep> -90.0</dep></entry>
  </interpolation>
    <center>
   <x-m>0.0</x-m>
   <y-m>-0.0</y-m>
   <z-m> 0.0</z-m>
  </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z>  0 </z>
    </axis>
  </animation>         
 
</PropertyList>
